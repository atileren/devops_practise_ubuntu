apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: prometheus-ui
  namespace: monitoring
  annotations:
    kubernetes.io/ingress.class: nginx

spec:
  rules:
  - host: {{prometheus_ingress_url}}
    http:
      paths:
      - backend:
          serviceName: prometheus-server-svc
          servicePort: 8080
